[user]
	email = vikthor.d@gmail.com
	name = victor Defontaine
[color]
	ui = true
	branch = auto
	diff = auto
	status = auto
  showbranch = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
	header = "#C9C9C9 normal dim"
	added = "yellow normal strike"
	updated = "green normal italic"
	changed = "#ff5900 normal italic"
	untracked = "#ff1b00 normal italic"
	nobranch = "white normal bold"
	branch = "#b900ff normal bold"
[log]
	date = short
[format]
	pretty = %Cred%h%Creset %C(blue)%an%Creset %C(magenta)%cd%Creset %C(yellow)%d%Creset %s 
[alias]
	s = status --porcelain
